generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model Client {
  id       String @id @default(uuid())
  nom      String
  postnom  String
  numero   String
  statut   Boolean
  quantite String
  email    String
}
